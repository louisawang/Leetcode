class Solution:
    # @return a tuple, (index1, index2)
    def twoSum(self, num, target):
        index1= 1
        n=len(num)
        index2= n
        remainer=0
        map={}
        for i in range(n):
            remainer=target-num[i]
            if remainer in map:
                index1 = map[remainer]+1
                index2 = i+1
                return (index1, index2)
                break
            map[num[i]]=i
        return None
                
